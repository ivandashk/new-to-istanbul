{
	"name": "new-to-istanbul",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "pnpm generate && vite dev",
		"build": "pnpm generate && pnpm sitemap && vite build",
		"preview": "pnpm generate && vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install",
		"generate": "node ./scripts/generate.mjs && pnpm format",
		"sitemap": "npx sitemap-generator-cli --filepath ./static/sitemap.xml --priority-map '1.0,0.8,0.6,0.4' --change-freq daily https://istanbul-guide.netlify.app"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.78",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.7",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"esbuild": "^0.15.10",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"mdsvex": "^0.10.6",
		"pnpm": "^7.13.2",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"rehype-figure": "^1.0.1",
		"remark-extract-frontmatter": "^3.2.0",
		"remark-frontmatter": "^4.0.1",
		"remark-parse": "^10.0.1",
		"remark-stringify": "^10.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"unified": "^10.1.2",
		"vite": "^3.1.0",
		"yaml": "^2.1.3"
	},
	"type": "module"
}
